<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="001" author="pnowak">
        <createTable tableName="SP_MEDIA">
            <column name="UUID" type="varchar(255)" autoIncrement="false">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="DTYPE" type="varchar(32)"/>
            <column name="FILENAME" type="varchar(255)">
                <!--<constraints nullable="false"/>-->
            </column>
            <column name="MIME_TYPE" type="varchar(50)"/>
            <column name="OWNER" type="varchar(255)"/>
            <column name="VISIBILITY" type="varchar(50)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>